# exclude any files that should not be uploaded to the webserver
- .DS_Store
- .listing
- .git*
- .rsync-excludes
- .lftp-mirror
- .env*
- .externalToolBuilders
- .project
- .settings
- build.ant
- composer.json
- composer.lock
- nbproject
- app/themes/underscore-hb/**.less
- app/themes/underscore-hb/bootstrap/less
- app/upgrade
- app/uploads
