# exclude any files that should not be uploaded to the webserver
- .DS_Store
- .listing
- .git*
- .rsync-excludes
- .lftp-mirror
- .env*
- .externalToolBuilders
- .project
- .settings
- build.xml
- composer.json
- composer.lock
- app/plugins/crayon-syntax-highlighter/log.txt
- app/upgrade
- app/uploads
