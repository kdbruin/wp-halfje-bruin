{
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpressNL",
        "type": "webroot",
        "version": "3.9.2",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/release/nl_NL/wordpress-3.9.2.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type": "vcs",
      "url": "ssh://filevault.localdomain/home/git/private/hb-plugin.git"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpressNL": "3.9.2",
    "fancyguy/webroot-installer": "1.1.0",
    "vlucas/phpdotenv": "1.0.6",
    "wpackagist-plugin/codestyling-localization": "1.99.30",
    "wpackagist-plugin/easy-fancybox": "1.5.6",
    "wpackagist-plugin/force-regenerate-thumbnails": "2.0.3",
    "wpackagist-plugin/get-the-image": "1.0.1",
    "wpackagist-plugin/post-snippets": "2.3.3",
    "wpackagist-plugin/series": "0.2.0",
    "wpackagist-plugin/wordpress-importer": "0.6.1",
    "kdb/hb-plugin": "dev-master",
    "wpackagist-theme/cazuela": "*",
    "wpackagist-theme/gumbo": "*",
    "wpackagist-theme/twentyfourteen": "*"
  },
  "extra": {
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"],
      "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpressNL"
  }
}
