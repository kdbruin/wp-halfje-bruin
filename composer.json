{
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpressNL",
        "type": "webroot",
        "version": "4.2.2",
        "dist": {
          "type": "zip",
          "url": "https://downloads.wordpress.org/release/nl_NL/wordpress-4.2.2.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    },
    {
      "type": "vcs",
      "url": "ssh://filevault.localdomain/home/git/private/hb-plugin.git"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpressNL": "4.2.2",
    "fancyguy/webroot-installer": "1.1.0",
    "vlucas/phpdotenv": "1.0.6",
    "wpackagist-plugin/classic-smilies": "1.3",
    "wpackagist-plugin/codestyling-localization": "1.99.30",
    "wpackagist-plugin/crayon-syntax-highlighter": "2.7.1",
    "wpackagist-plugin/disqus-comment-system": "2.84",
    "wpackagist-plugin/enable-media-replace": "3.0.3",
    "wpackagist-plugin/force-regenerate-thumbnails": "2.0.5",
    "wpackagist-plugin/post-snippets": "2.3.5",
    "wpackagist-plugin/series": "0.2.0",
    "wpackagist-plugin/wordpress-importer": "0.6.1",
    "wpackagist-plugin/wp-db-backup": "2.2.4",
    "wpackagist-plugin/advanced-custom-fields": "4.4.2",
    "wpackagist-plugin/ricg-responsive-images": "2.3.1",
    "wpackagist-plugin/shortcodes-ultimate": "4.9.7",
    "kdb/hb-plugin": "dev-master",
    "wpackagist-theme/gridsby": "*"
  },
  "extra": {
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"],
      "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpressNL"
  }
}
