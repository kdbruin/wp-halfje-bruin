{
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpressNL",
        "type": "webroot",
        "version": "3.8.2",
        "dist": {
          "type": "zip",
          "url": "http://nl.wordpress.org/wordpress-3.8.2-nl_NL.zip"
        },
        "require" : {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpressNL": "3.8.2",
    "fancyguy/webroot-installer": "1.1.0",
    "vlucas/phpdotenv": "1.0.6",
    "wpackagist-plugin/codestyling-localization": "1.99.30",
    "wpackagist-plugin/carousel-without-jetpack": "0.6",
    "wpackagist-plugin/font-awesome-4-menus": "4.0.3.0",
    "wpackagist-plugin/force-regenerate-thumbnails": "2.0.3",
    "wpackagist-plugin/get-the-image": "0.9.0",
    "wpackagist-plugin/image-watermark": "1.3.1",
    "wpackagist-plugin/portfolio-post-type": "0.8.1",
    "wpackagist-plugin/post-snippets": "2.3.2",
    "wpackagist-plugin/series": "0.2.0",
    "wpackagist-plugin/wordpress-importer": "0.6.1",
    "wpackagist-theme/base-wp": "*",
    "wpackagist-theme/cazuela": "*",
    "wpackagist-theme/portfolio-press": "*",
    "wpackagist-theme/twentyfourteen": "*"
  },
  "extra": {
    "installer-paths": {
      "app/plugins/{$name}/": ["type:wordpress-plugin"],
      "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpressNL"
  }
}
